<xml xmlns="https://developers.google.com/blockly/xml">
  <block type="start_hat" id="start" x="40" y="40">
    <next>
      <block type="robot_set_speed" id="spd">
        <value name="SPEED">
          <block type="math_number" id="n100">
            <field name="NUM">100</field>
          </block>
        </value>
        <next>
          <block type="controls_forever" id="forever">
            <statement name="DO">
              <block type="controls_if" id="if_edge">
                <value name="IF0">
                  <block type="logic_operation" id="or_edge">
                    <field name="OP">OR</field>
                    <value name="A">
                      <block type="logic_compare" id="cmpL">
                        <field name="OP">LT</field>
                        <value name="A">
                          <block type="sensor_get" id="lightL">
                            <field name="TYPE">LIGHT</field>
                            <field name="SENS">0</field>
                          </block>
                        </value>
                        <value name="B">
                          <block type="math_number" id="thrL">
                            <field name="NUM">35</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <value name="B">
                      <block type="logic_compare" id="cmpR">
                        <field name="OP">LT</field>
                        <value name="A">
                          <block type="sensor_get" id="lightR">
                            <field name="TYPE">LIGHT</field>
                            <field name="SENS">1</field>
                          </block>
                        </value>
                        <value name="B">
                          <block type="math_number" id="thrR">
                            <field name="NUM">35</field>
                          </block>
                        </value>
                      </block>
                    </value>
                  </block>
                </value>
                <statement name="DO0">
                  <block type="robot_move" id="back">
                    <value name="L">
                      <block type="math_number" id="m80L">
                        <field name="NUM">-80</field>
                      </block>
                    </value>
                    <value name="R">
                      <block type="math_number" id="m80R">
                        <field name="NUM">-80</field>
                      </block>
                    </value>
                    <next>
                      <block type="wait_seconds" id="w1">
                        <value name="SECONDS">
                          <block type="math_number" id="t025">
                            <field name="NUM">0.25</field>
                          </block>
                        </value>
                        <next>
                          <block type="robot_turn_timed" id="turn1">
                            <field name="DIR">LEFT</field>
                            <value name="SEC">
                              <block type="math_number" id="t035">
                                <field name="NUM">0.35</field>
                              </block>
                            </value>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </statement>
                <next>
                  <block type="controls_if" id="if_push">
                    <value name="IF0">
                      <block type="logic_compare" id="cmpDist">
                        <field name="OP">LT</field>
                        <value name="A">
                          <block type="sensor_get" id="dist">
                            <field name="TYPE">DIST</field>
                            <field name="SENS">2</field>
                          </block>
                        </value>
                        <value name="B">
                          <block type="math_number" id="thrD">
                            <field name="NUM">45</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <statement name="DO0">
                      <block type="robot_move" id="push">
                        <value name="L">
                          <block type="math_number" id="p100L">
                            <field name="NUM">100</field>
                          </block>
                        </value>
                        <value name="R">
                          <block type="math_number" id="p100R">
                            <field name="NUM">100</field>
                          </block>
                        </value>
                      </block>
                    </statement>
                    <statement name="ELSE">
                      <block type="robot_move" id="search">
                        <value name="L">
                          <block type="math_number" id="s55L">
                            <field name="NUM">55</field>
                          </block>
                        </value>
                        <value name="R">
                          <block type="math_number" id="s55R">
                            <field name="NUM">-55</field>
                          </block>
                        </value>
                      </block>
                    </statement>
                  </block>
                </next>
              </block>
            </statement>
          </block>
        </next>
      </block>
    </next>
  </block>
</xml>
